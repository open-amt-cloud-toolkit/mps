{
  "name": "mps-microservice",
  "version": "0.0.1",
  "description": "Containerized MPS service",
  "scripts": {
    "build": "tsc",
    "compile": "tsc",
    "lint": "eslint . --ext .ts",
    "start": "npm run build && node ./dist/index.js",
    "test": "jest --detectOpenHandles --forceExit",
    "dev": "set MPS_USE_VAULT=false&& set MPS_LOG_LEVEL=silly&&set MPS_AUTH_ENABLED=false&& npm start",
    "devx": "MPS_USE_VAULT=false MPS_LOG_LEVEL=silly MPS_AUTH_ENABLED=false npm start"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "node-forge": "^0.10.0",
    "node-vault": "^0.9.21",
    "winston": "^3.0.0",
    "rc": "^1.2.8",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.28",
    "@types/node-forge": "^0.9.7",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "26.6.3",
    "jest-transform-stub": "^2.0.0",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
